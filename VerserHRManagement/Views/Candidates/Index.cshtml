                $("#image").hide();
                $("#previewImage").show();
            }
            else {
                $("#image").show();
                $("#previewImage").hide();
            }


            $('#mainTable tbody').on('click', 'tr', function () {
                if ($(this).hasClass('selected')) {
                    $(this).removeClass('selected');
                }
                else {
                    $('#mainTable').DataTable().$('tr.selected').removeClass('selected');
                    $(this).addClass('selected');
                }
            });

