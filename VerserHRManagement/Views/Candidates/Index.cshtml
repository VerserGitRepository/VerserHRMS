    function LoadRating(id) {
			$("#ratingdiv").empty();
			$("#ratingheader").empty();
			$("#ratingtotal").empty();
			$("#divspan5").css('width', 0);
			$("#divspan4").css('width', 0);
			$("#divspan3").css('width', 0);
			$("#divspan2").css('width', 0);
			$("#divspan1").css('width', 0);

			$.ajax({
				type: "GET",
				url: './GetResourceRating',
				contentType: "application/json; charset=utf-8",
				data: {'candidateId':id},
				datatype: "json",
				success: function (result) {
					for (var i = 0; i < result.AverageRating ; i++) {
						$("#ratingdiv").append('</span><span class="glyphicon glyphicon-star"></span>');
					}
					for (var i = result.AverageRating; i < 5; i++) {
						$("#ratingdiv").append('</span><span class="glyphicon glyphicon-star-empty"></span>');
					}
					var fiverating = ((result.FiveRating / result.TotalRating) * 100) + '%';
					var fourrating = ((result.FourRating / result.TotalRating) * 100) + '%';
					var threerating = ((result.ThreeRating / result.TotalRating) * 100) + '%';
					var tworating = ((result.TwoRating / result.TotalRating) * 100) + '%';
					var onerating = ((result.OneRating / result.TotalRating) * 100) + '%';
					$("#ratingheader").append(result.AverageRating);
					$("#ratingtotal").text("    "+result.TotalRating+" - total");
					$("#divspan5").css('width', fiverating);
					$("#divspan4").css('width', fourrating);
					$("#divspan3").css('width', threerating);
					$("#divspan2").css('width', tworating);
					$("#divspan1").css('width', onerating);

				   // alert('test');
				}
			});
		}

