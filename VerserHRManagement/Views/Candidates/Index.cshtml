                $("#reportingDiv").show();
            }
            else {
                $("#reportingDiv").hide();
            }
           // alert(resourceReporting);
            $("#txtfName").val(CandidateName.split(' ')[0]);
            $("#txtlName").val(CandidateName.split(' ')[1]);
            $("#txtAddress1").val(Address);
            $("#ddlInactiveReason").val(inactivereason);
            $("#txtAddress2").val(Address);
            $("#txtCity").val(City);
            $("#txtState").val(state);
            $("#txtPostalCode").val(postcode);
            $("#txtEmail").val(Email);
