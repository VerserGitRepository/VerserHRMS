@model VerserHRManagement.Models.Candidate
@using VerserHRManagement.HelperServices;
<script src="~/Scripts/jquery-3.4.1.min.js" type="text/javascript"></script>
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<script src="~/Scripts/jquery-3.4.1.min.js" type="text/javascript"></script>
<link href="https://cdn.datatables.net/1.10.20/css/jquery.dataTables.min.css" rel="stylesheet" />
<script src="https://code.jquery.com/jquery-3.3.1.js"></script>
<script src=" https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js"></script>
<script src="https://cdn.datatables.net/1.10.15/js/dataTables.bootstrap4.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
<link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.17.47/css/bootstrap-datetimepicker.min.css" rel="stylesheet" />
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.2/jquery-confirm.min.css">
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.2/jquery-confirm.min.js"></script>
<link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet">
<script src="https://gitcdn.github.io/bootstrap-toggle/2.2.2/js/bootstrap-toggle.min.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.2/jquery-confirm.min.css">
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.2/jquery-confirm.min.js"></script>
<script src='https://kit.fontawesome.com/a076d05399.js'></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<script src="~/Scripts/bootstrap-multiselect.js"></script>
<script src="~/Scripts/scrollForever.js"></script>
<link href="~/Content/bootstrap-multiselect.css" rel="stylesheet" />
<link href="~/Content/scroll.css" rel="stylesheet" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.0.4/popper.js"></script>
<div class="container-fluid" style="margin-left:0px;padding-top:0px">
    <div class="container" style="width:170%; padding-top:15px; margin-left:0px; border-color:lightgray;padding-left:0px">
        <div class="col-md-4">
            <table class="table table-bordered  table-responsive table-hover table-dark" id="mainTable" style="margin-bottom:2Px">
                <thead style="background-color: #428bca; color:white">
                    <tr>
                        <th>
                            @Html.DisplayName("CANDIDATES")
                        </th>
                        <th>
                            @Html.DisplayName("Skills")
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.CandidateList)
                    {
                        <tr>
                            <td onclick="LoadAddressData(@item.ID,'@item.CandidateName','@item.DateCreated','@item.Address','@item.City','@item.state','@item.postcode','@item.Email','@item.Phone',
                                '@if(item.profilePicture != null) { @Convert.ToBase64String(@item.profilePicture)}','@if (item.DOB.HasValue) {@item.DOB.Value.ToString("yyyy-MM-dd")}','@item.Age','@item.Gender','@item.MaritalStatus',@Json.Encode(item.isactive),
                                '@item.WarehouseID','@item.ResourceCategoriesID','@item.DrivingLicenseID','@item.fK_ResourceInActiveReasonID',@Json.Encode(@item.ResourceReporting.Select(c=> c.Value).ToArray()));
                                LoadEmployment(@item.ID,'@item.PayFrequenciesID','@item.ADP_EmployeeID','@item.AssignResourcesID','@item.Annualsalary','@item.HourlyRate','@item.DailyRate','@item.WorkRightID',
                                '@item.EmployeeStatusSetID','@item.EmploymentTypesID','@item.NoticePeriod','@Json.Encode(item.isPeopleManager)',@Json.Encode(@item.ResourceReporting.Select(c=> c.Id).ToArray()));
                                LoadDocuments(@item.ID,'@item.FilePath','@item.certificate1','@item.DriverLicense','@item.PoliceCheckReport','@item.Visa','@item.SuperChoice','@item.TFNDeclaration',
                                '@item.BankDetails','@item.CodeOFConduct','@item.WHS');
                                LoadSkills(@item.ID,'@item.CandidateSkills','@item.Experience','@item.JobTitle','@item.technicalLevelsID','@item.RateOfSkillExperties');
                                LoadRating(@item.ID);
                                LoadAccessAndDevices(@Json.Encode(@item.CandidateAllcaotedResouces));">
                                <font style="font-weight:800;color:#515151">
                                    @Html.DisplayFor(modelItem => item.CandidateName)
                                    <input type="hidden" value="@item.ID" id="hiddenModelId" />
                                    <input type="hidden" value='@Server.MapPath("~/UploadedFiles")' id="hiddenPath" />
                                </font> <font style="font-size:xx-small"> @item.Address @item.City @item.state @item.postcode </font> <br />
                            </td>
                            <td>@item.CandidateSkills</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
        <div class="col-md-7" style="margin-left:30px">
            <div class="panel with-nav-tabs panel-default" style="width:170%;margin-bottom:0px">
                <div class="panel-heading">
                    <ul class="nav nav-tabs">
                        <li class="active"><a href="#tab1default" data-toggle="tab">PERSONAL</a></li>
                        <li><a href="#tab2default" data-toggle="tab">EMPLOYMENT</a></li>
                        <li><a href="#tab3default" data-toggle="tab">SKILLS</a></li>
                        <li><a href="#tab5default" data-toggle="tab">PERFORMANCE</a></li>

                        @if (UserRoles.UserCanEdit() == true)
                        {
                            <li><a href="#tab4default" data-toggle="tab">DOCUMENTS</a></li>
                            <li><a href="#tab6default" data-toggle="tab">OFFER</a></li>
                            <li><a href="#tab7default" data-toggle="tab">POLICIES</a></li>
                            <li><a href="#tab8default" data-toggle="tab">ACCESS & DEVICES</a></li>
                            <li><a href="#tab9default" data-toggle="tab">OFF-BOARD</a></li>
                            <li class="pull-right" style="margin-right:20px;vertical-align:bottom">
                                @using (Html.BeginForm("ExportTimesSheetToExcel", "Candidates", FormMethod.Post))
                                {
                                    <input type="image" src="~/Images/excel.png" alt="Submit" width="20" height="20">
                                }
                            </li>
                        }
                    </ul>
                </div>
                <div class="panel-body" style="margin-bottom:0px">
                    <div class="tab-content" style="margin-bottom:0px">
                        <div class="tab-pane fade in active" id="tab1default" style="margin-bottom:0px;margin-top:0px">
                            <div class="col-md-6" style="padding-left:0px;padding-bottom:10px;margin-right:10px">
                                <div class="row">
                                    <form>
                                        <div class="container table-bordered" style="margin:20px; width:900px;padding-top:0px;padding-bottom:-20px;margin-top:0px">
                                            <div class="col-md-8" style="margin-right:2px">
                                                <div class="form-group row">
                                                    <div class="col-md-4">
                                                        <div class="col-sm-12">
                                                            <img class="preview-img" src="http://simpleicon.com/wp-content/uploads/account.png" alt="Preview Image" width="250" height="150" id="previewImage" onchange="readURL(this);" />
                                                            <img id="image" class="image-border form-control form-control-sm" width="250" height="150">
                                                            <input type="file" id="imgupload" style="display:none" onchange="readURL(this);" />
                                                        </div>
                                                    </div>

                                                </div>
                                                <div class="row" style="margin-left:5px;">
                                                    <div class="form-group row">
                                                        <div class="col-md-5" style="margin-right:0px">
                                                            <label>First Name</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <i class="glyphicon glyphicon-user"></i>
                                                                </span>  <input type="text" id="txtfName" class="form-control" placeholder="">
                                                            </div>

                                                        </div>
                                                        <div class="col-md-4">
                                                            <label>Last Name</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <i class="glyphicon glyphicon-user"></i>
                                                                </span> <input type="text" id="txtlName" class="form-control" placeholder="">
                                                            </div>


                                                        </div>
                                                        <div class="col-sm-1">
                                                            <label>DOB</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <i class="glyphicon glyphicon-calendar"></i>
                                                                </span> <input type="date" class="col-xs-1 form-control" id="txtDOB" placeholder="DOB" onchange="calculateAge(this);">
                                                            </div>

                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <div class="col-md-3">
                                                            <label>Age</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <i class="glyphicon glyphicon-calendar"></i>
                                                                </span> <input type="text" class="form-control" id="txtAge" placeholder="Age">
                                                            </div>

                                                        </div>
                                                        <div class="col-md-4">
                                                            <label>Gender</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <i class="fas fa-user-friends"></i>
                                                                </span>
                                                                <select class="form-control" id="txtGender">
                                                                    <option>Select</option>
                                                                    <option>Male</option>
                                                                    <option>Female</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-3">
                                                            <label>Marital Status</label>
                                                            <select class="form-control" id="txtStatus">
                                                                <option>Married</option>
                                                                <option>Single</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <div class="col-md-4">
                                                            <label>Warehouse:</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <i class="fas fa-warehouse"></i>
                                                                </span> @Html.DropDownListFor(model => model.WarehouseID, Model.WarehouseNameList, "WAREHOUSE", new { @class = "form-control", id = "ddlWarehouseId" })
                                                            </div>

                                                        </div>
                                                        <div class="col-md-4">
                                                            <label>Categories:</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <i class="fas fa-user-tie"></i>
                                                                </span> @Html.DropDownListFor(model => model.ResourceCategoriesID, Model.ResourceCategoriesList, "Resource Categories", new { @class = "form-control", id = "ddlResourcesCatId" })
                                                            </div>

                                                        </div>
                                                        <div class="col-md-4">
                                                            <label>Driving License:</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <i class="fas fa-truck"></i>
                                                                </span>  @Html.DropDownListFor(model => model.DrivingLicenseID, Model.DrivingLicensesList, "Driver License", new { @class = "form-control", id = "ddlDrivingLicenseID" })
                                                            </div>

                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <label>Active</label>
                                                        <div class="input-group">
                                                            <input type="checkbox" checked data-toggle="toggle" data-onstyle="info" id="chkIsActive" onchange="ResourceChange(this);">
                                                        </div>
                                                        @*</div>*@
                                                        @*<div class="col-md-3">*@
                                                        <label></label>
                                                        @Html.DropDownListFor(Model => Model.InactiveReason, new List<SelectListItem>
                                                            { new SelectListItem {Text="Resigned",Value="1" },
                                                            new SelectListItem {Text="Unsuitable",Value="2" },
                                                            new SelectListItem {Text="Lapsed",Value="3" },
                                                            }, "InActive Reason", new { @class = "form-control", @id = "ddlInactiveReason", @onchange = "UpdateInactiveReason();" })
                                                        @Html.ValidationMessageFor(model => model.InactiveReason, "", new { @class = "text-danger" })
                                                        @*</div>*@
                                                    </div>
                                                    <div class="col-sm-12 col-md-8 pull-right">
                                                        <div class="table table-bordered pull-right" style="margin-bottom:0px">
                                                            <div class="row" style="height:0%">
                                                                <div class="col-xs-12 col-md-6 text-center">
                                                                    <h1 class="rating-num" id="ratingheader"></h1>
                                                                    <div class="rating" id="ratingdiv">

                                                                    </div>
                                                                    <div>
                                                                        <span id="ratingtotal" class="glyphicon glyphicon-user"></span>
                                                                    </div>
                                                                </div>

                                                                <div class="col-xs-12 col-md-6">
                                                                    <div class="row rating-desc">
                                                                        <div style="margin-bottom:2px">
                                                                            <div class="col-xs-4 col-md-3 text-right" style="font-size:8px">
                                                                                <span class="glyphicon glyphicon-star" style="font-size:6px"></span>5
                                                                            </div>
                                                                            <div class="col-xs-8 col-md-9">
                                                                                <div class="progress progress-striped">
                                                                                    <div id="divspan5" class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="20"
                                                                                         aria-valuemin="0" aria-valuemax="100">
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <!-- end 5 -->
                                                                        <div style="margin-bottom:2px">
                                                                            <div class="col-xs-4 col-md-3 text-right" style="font-size:8px">
                                                                                <span class="glyphicon glyphicon-star" style="font-size:6px"></span>4
                                                                            </div>
                                                                            <div class="col-xs-8 col-md-9">
                                                                                <div class="progress">
                                                                                    <div id="divspan4" class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="20"
                                                                                         aria-valuemin="0" aria-valuemax="100">
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <!-- end 4 -->
                                                                        <div class="col-xs-4 col-md-3 text-right" style="font-size:8px">
                                                                            <span class="glyphicon glyphicon-star" style="font-size:6px"></span>3
                                                                        </div>
                                                                        <div class="col-xs-8 col-md-9">
                                                                            <div class="progress">
                                                                                <div id="divspan3" class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="20"
                                                                                     aria-valuemin="0" aria-valuemax="100">
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <!-- end 3 -->
                                                                        <div class="col-xs-3 col-md-3 text-right" style="font-size:8px">
                                                                            <span class="glyphicon glyphicon-star" style="font-size:6px"></span>2
                                                                        </div>
                                                                        <div class="col-xs-8 col-md-9">
                                                                            <div class="progress">
                                                                                <div id="divspan2" class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="20"
                                                                                     aria-valuemin="0" aria-valuemax="100">
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <!-- end 2 -->
                                                                        <div class="col-xs-3 col-md-3 text-right" style="font-size:8px">
                                                                            <span class="glyphicon glyphicon-star" style="font-size:6px"></span>1
                                                                        </div>
                                                                        <div class="col-xs-8 col-md-9">
                                                                            <div class="progress">
                                                                                <div id="divspan1" class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="80"
                                                                                     aria-valuemin="0" aria-valuemax="100">
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <!-- end 1 -->
                                                                    </div>
                                                                    <!-- end row -->
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                            <div class="list-group pull-right" id="feature" style="padding-top:10PX;padding-right:5px; width:190px;height:250px;overflow-y:scroll;">

                                                <a href="#" class="list-group-item list-group-item-action active">MONTHLY BIRTHDAY'S</a>

                                                @foreach (var Resourcereportings in Model.CurrentBirthdays)
                                                {
                                                    <a href="#" class="list-group-item list-group-item-action">@Resourcereportings</a>
                                                }
                                            </div>
                                            <div class="list-group pull-right" style="padding-top:10PX;padding-right:5px; width:190px;height:250px;overflow-y:scroll;" id="reportingDiv">
                                            </div>
                                        </div>
                                    </form>
                                </div>
                                <div class="row grid-divider">
                                    <div class="row" style="margin-left:40px;margin-bottom:0px">
                                        <fieldset style="margin-bottom:0px">
                                            <div class="form-group row" style="margin-bottom:-20px">
                                                <div class="container table-bordered pull-left" @*style="margin:20px; margin-top:0px;width:800px;padding-top:5px;padding-bottom:5px"*@>
                                                    @*<div class="row">*@
                                                    <div class="form-group row">
                                                        <div class="col-md-4" style="margin-left:5px">
                                                            <label>Address:</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <i class="glyphicon glyphicon-home"></i>
                                                                </span><input id="txtAddress1" placeholder="Address Line 1" class="form-control" required="true" value="" type="text">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-2">
                                                            <label>City:</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <i class="fas fa-map-marker-alt"></i>
                                                                </span><input id="txtCity" placeholder="Suburb" class="form-control" required="true" value="" type="text">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-2">
                                                            <label>State:</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <i class="glyphicon glyphicon-home"></i>
                                                                </span><input id="txtState" placeholder="State" class="form-control" required="true" value="" type="text">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-2">
                                                            <label>Postal Code:</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <i class="glyphicon glyphicon-home"></i>
                                                                </span><input id="txtPostalCode" placeholder="PostCode" class="form-control" required="true" value="" type="text">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <div class="col-md-3">
                                                            <label>Email:</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <i class="glyphicon glyphicon-envelope"></i>
                                                                </span><input id="txtEmail" placeholder="Email" class="form-control" required="true" value="" type="text">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-3">
                                                            <label>Mobile</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <i class="fas fa-mobile-alt" style="font-size:20px"></i>
                                                                </span><input id="txtMobile" placeholder="Phone Number" class="form-control" required="true" value="" type="text">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-3">
                                                            <label>Phone</label>

                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <i class="glyphicon glyphicon-earphone"></i>
                                                                </span><input id="txtPhoneNumber" placeholder="Phone Number" class="form-control" required="true" value="" type="text">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="tab-pane fade" id="tab2default">
                            <div class="row" style="margin-left:10px">
                                <div class="form-group row">
                                    <div class=" row col-md-6 well" style="width:370px;height:390px">
                                        <div class="col-md-12" style="margin-top:15px;">
                                            <label class="col-sm-9 col-form-label col-form-label-sm">Pay Frequency</label>
                                            @Html.DropDownListFor(model => model.PayFrequenciesID, Model.PayFrequencyList, "Pay Frequency", new { @class = "form-control", id = "ddlPayFrequencyId" })
                                        </div>
                                        <div class="col-md-12" style="margin-top:15px;">
                                            <label class="col-sm-9 col-form-label col-form-label-sm">ADP Employee Id</label>
                                            <input type="text" id="txtADPEmployeeId" class="form-control form-control-sm" placeholder="ADP Employee Id">
                                        </div>
                                        <div class="col-md-12" style="margin-top:15px;">
                                            <label class="col-sm-9 col-form-label col-form-label-sm">Resource Assigned</label>
                                            @Html.DropDownListFor(model => model.AssignResourcesID, Model.AssignResourceList, "AssignResource", new { @class = "form-control", id = "ddlResourceAssigned" })
                                        </div>

                                        <div class="col-md-12" style="margin-top:15px;">
                                            <label class="col-sm-9 col-form-label col-form-label-sm">Annual Salary</label>
                                            <input type="text" class="form-control form-control-sm" id="txtAnnualSalary" placeholder="Annual Salary">
                                        </div>
                                        <div class="col-md-12" style="margin-top:15px;">
                                            <label class="col-sm-9 col-form-label col-form-label-sm">Hourly Rate</label>
                                            <input type="text" class="form-control form-control-sm" id="txtHourlyRate" placeholder="Hourly Rate">
                                        </div>
                                    </div>
                                    <div class=" row col-md-6 well" style="width:370px;height:390px">
                                        <div class="col-md-12" style="margin-top:15px;">
                                            <label class="col-sm-9 col-form-label col-form-label-sm">Daily Rate</label>
                                            <input type="text" class="form-control form-control-sm" id="txtDailyRate" placeholder="Daily Rate">
                                        </div>
                                        <div class="col-md-12" style="margin-top:15px;">
                                            <label class="col-sm-9 col-form-label col-form-label-sm">Work Rights</label>
                                            @Html.DropDownListFor(model => model.WorkRights, Model.WorkRightsList, "Works Rights", new { @class = "form-control", id = "ddlWorkRights" })
                                        </div>
                                        <div class="col-md-12" style="margin-top:15px;">
                                            <label class="col-sm-9 col-form-label col-form-label-sm">Employment Status</label>
                                            @Html.DropDownListFor(model => model.EmployeeStatusSetID, Model.EmployeeStatusList, "Employment Status", new { @class = "form-control", id = "ddlEmployeeStatusId" })
                                        </div>
                                        <div class="col-md-12" style="margin-top:15px;">
                                            <label class="col-sm-9 col-form-label col-form-label-sm">Employment Type</label>
                                            @Html.DropDownListFor(model => model.EmploymentTypesID, Model.EmploymentList, "Employment Type", new { @class = "form-control", id = "ddlEmploymentType" })
                                        </div>
                                        <div class="col-md-12" style="margin-top:15px;">
                                            <label class="col-sm-9 col-form-label col-form-label-sm">Notice Period</label>
                                            <input type="text" class="form-control form-control-sm" id="txtNoticePeriod" placeholder="Notice Period">
                                        </div>
                                    </div>
                                    <div class=" row col-md-6 well" style="width:250px;height:390px">

                                        <div class="col-md-12" style="margin-top:15px;">
                                            <label>People Manager</label>
                                            <div class="input-group">
