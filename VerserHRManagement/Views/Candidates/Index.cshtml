@model VerserHRManagement.Models.Candidate
@using VerserHRManagement.HelperServices;

<link href="~/Content/bootstrap.css" rel="stylesheet" />
<link href="~/Content/bootstrap-multiselect.css" rel="stylesheet" />
<link href="~/Content/scroll.css" rel="stylesheet" />
<link href="https://cdn.datatables.net/1.10.20/css/jquery.dataTables.min.css" rel="stylesheet" />
<link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.17.47/css/bootstrap-datetimepicker.min.css" rel="stylesheet" />
<link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.2/jquery-confirm.min.css">
<script src=" https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/bootstrap-multiselect.js"></script>
<script src="~/Scripts/jquery-3.4.1.min.js" type="text/javascript"></script>
<script src="~/Scripts/scrollForever.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js"></script>
<script src="https://cdn.datatables.net/1.10.15/js/dataTables.bootstrap4.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.2/jquery-confirm.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.0.4/popper.js"></script>
<script src="https://code.jquery.com/jquery-3.3.1.js"></script>
<script src="https://gitcdn.github.io/bootstrap-toggle/2.2.2/js/bootstrap-toggle.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
<script src='https://kit.fontawesome.com/a076d05399.js'></script>

<div class="container-fluid">

    <nav class="col-md-3 d-none d-md-block bg-light sidebar" style="padding-right:2px; margin-top:10px;">
        <div class="sidebar-sticky">
            <table class="table table-bordered  table-responsive table-hover table-dark display nowrap" id="mainTable" style="width:100%">
                <thead style="background-color: orange; color:white">
                    <tr>
                        <th style="border:none">
                            @Html.DisplayName("Candidates")
                        </th>
                        <th style="border:none">
                            @Html.DisplayName("Skills")
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.CandidateList)
                    {
                        <tr>
                            <td onclick=" LoadUserActiveStatus('@Json.Encode(@item.isactive)'), LoadAddressData(@item.ID, '@item.CandidateName', '@item.DateCreated', '@item.Address',
     '@item.City', '@item.state', '@item.postcode', '@item.Email', '@item.Phone', '@if(item.profilePicture != null) { @Convert.ToBase64String(@item.profilePicture)}',
     '@if (item.DOB.HasValue) {@item.DOB.Value.ToString("yyyy-MM-dd")}', '@item.Age', '@item.Gender', '@item.MaritalStatus', '@Json.Encode(item.isactive)',
     '@item.WarehouseID', '@item.ResourceCategoriesID', '@item.DrivingLicenseID', '@item.fK_ResourceInActiveReasonID',
     '@Json.Encode(@item.ResourceReporting.Select(c=> c.Value).ToArray())', '@item.EmployementTypeId', '@item.HourlyRate', '@item.DailyRate', '@item.NoticePeriod',
     '@item.payFrequenciesID', '@item.ADP_EmployeeID', '@item.WorkRightID', '@item.AssignResourcesID',
     '@item.EmployeeStatusSetID', '@item.Annualsalary', '@Json.Encode(item.isPeopleManager)', '@Json.Encode(@item.ResourceReporting.Select(c=> c.Id).ToArray())',
     '@item.CandidateSkills', '@item.Experience', '@item.JobTitle','@item.technicalLevelsID', '@item.RateOfSkillExperties'); ">

                                <font style="font-weight:800;color:#515151">
                                    @Html.DisplayFor(modelItem => item.CandidateName)
                                    <input type="hidden" value='@Server.MapPath("~/UploadedFiles")' id="hiddenPath" />
                                </font>
                                <font style="font-size:xx-small"> @item.Address @item.City @item.state @item.postcode </font>
                                <br />
                            </td>
                            <td>@item.CandidateSkills</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </nav>


    <main role="main" class="col-md-9 pull-right" style="padding-left:0">
        <div class="panel with-nav-tabs panel-default">
            <div class="panel-heading">
                <ul class="nav nav-tabs">
                    <li class="active">
                        <a href="#tab1default" data-toggle="tab">Employment</a>
                    </li>
                    <li>
                        <a href="#tab2default" data-toggle="tab">Personal</a>
                    </li>
                    <li>
                        <a href="#tab3default" data-toggle="tab">Verser Skills</a>
                    </li>
                    <li>
                        <a href="#tab5default" data-toggle="tab">Performance</a>
                    </li>

                    @if (UserRoles.UserCanEdit() == true)
                    {
                        @*<li><a href="#tab4default" data-toggle="tab">Documents</a></li>*@
                        <li>
                            <a href="#tab7default" data-toggle="tab">Documents and Policies</a>
                        </li>
                        <li>
                            <a href="#tab6default" data-toggle="tab">Offer</a>
                        </li>

                        <li>
                            <a href="#tab8default" data-toggle="tab">Access & Devices</a>
                        </li>
                        <li>
                            <a href="#tab9default" data-toggle="tab">Off-board</a>
                        </li>
                        <li class="pull-right" style="margin-right:20px;vertical-align:bottom; padding:10px 15px">
                            @using (Html.BeginForm("ExportTimesSheetToExcel", "Candidates", FormMethod.Post))
                            {
                                <input type="image" src="~/Images/excel.png" alt="Submit" style="width:20px; height:20px">
                            }
                        </li>
                    }
                </ul>
            </div>

            <div class="panel-body">
                <div class="tab-content">

                    <div class="tab-pane fade in active" id="tab1default">
                        <div class="row">
                            <form>
                                <div class="col-md-9">
                                    <div class="form-group row">
                                        <div class="col-md-4">
                                            <div class="col-sm-12">

                                                <img class="preview-img" src="http://simpleicon.com/wp-content/uploads/account.png" alt="Preview Image" width="250" height="150" id="previewImage" onchange="readURL(this);" />

                                                <img id="image" class="image-border form-control form-control-sm" width="250" height="150">

                                                <input type="file" id="imgupload" style="display:none" onchange="readURL(this);" />
                                                <input type="hidden" id="hiddenModelId" />
                                            </div>
                                        </div>

                                    </div>
                                    <div class="row well" style="margin-bottom:0 !important">
                                        <div class="form-group row">
                                            <div class="col-md-4" style="margin-right:0px">
                                                <label>First Name</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <i class="glyphicon glyphicon-user"></i>
                                                    </span>
                                                    <input type="text" id="txtfName" class="form-control" placeholder="">
                                                </div>
                                            </div>

                                            <div class="col-md-4">
                                                <label>Last Name</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <i class="glyphicon glyphicon-user"></i>
                                                    </span>
                                                    <input type="text" id="txtlName" class="form-control" placeholder="">
                                                </div>
                                            </div>

                                            <div class="col-md-4">
                                                <label>Employment Type</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <i class="glyphicon glyphicon-briefcase"></i>
                                                    </span>@Html.DropDownListFor(model => model.EmploymentTypesID, Model.EmploymentList, "Employment Type", new { @class = "form-control", id = "ddlEmploymentTypeId" })
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group row">
                                            <div class="col-md-4">
                                                <label>Warehouse</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <i class="fas fa-warehouse"></i>
                                                    </span> @Html.DropDownListFor(model => model.WarehouseID, Model.WarehouseNameList, "WAREHOUSE", new { @class = "form-control", id = "ddlWarehouseId" })
                                                </div>

                                            </div>
                                            <div class="col-md-4">
                                                <label>Categories</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <i class="fas fa-user-tie"></i>
                                                    </span> @Html.DropDownListFor(model => model.ResourceCategoriesID, Model.ResourceCategoriesList, "Resource Categories", new { @class = "form-control", id = "ddlResourcesCatId" })
                                                </div>

                                            </div>
                                            <div class="col-md-4">
                                                <label>Driving License</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <i class="fas fa-truck"></i>
                                                    </span>  @Html.DropDownListFor(model => model.DrivingLicenseID, Model.DrivingLicensesList, "Driver License", new { @class = "form-control", id = "ddlDrivingLicenseID" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <div class="col-md-4">
                                                <label>Email:</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <i class="glyphicon glyphicon-envelope"></i>
                                                    </span>
                                                    <input id="txtEmail" placeholder="Email" class="form-control" required="true" value="" type="text">
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <label>Mobile</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <i class="fas fa-mobile-alt" style="font-size:20px"></i>
                                                    </span>
                                                    <input id="txtMobile" placeholder="Phone Number" class="form-control" required="true" value="" type="text">
                                                </div>
                                            </div>

                                            <div class="col-md-4">
                                                <label>Hourly Rate</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <i class="glyphicon glyphicon-usd"></i>
                                                    </span>
                                                    <input type="text" id="txtHourlyRate" class="form-control" placeholder="Hourly Rate">
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-4" style="padding:0; margin-bottom:10px">

                                            <div class="input-group">
                                                <input type="checkbox" @*checked*@ data-toggle="toggle" data-onstyle="warning" id="chkIsActive" onchange="ResourceChange(this);">
                                                &nbsp;
                                                <label id="lblActiveState" />
                                            </div>
                                            <label></label>
                                            @Html.DropDownListFor(Model => Model.InactiveReason, new List<SelectListItem>
                                                { new SelectListItem {Text="Resigned",Value="1" },
                                                new SelectListItem {Text="Unsuitable",Value="2" },
                                                new SelectListItem {Text="Lapsed",Value="3" },
                                                }, "InActive Reason", new { @class = "form-control", @id = "ddlInactiveReason", @onchange = "UpdateInactiveReason();" })
                                            @Html.ValidationMessageFor(model => model.InactiveReason, "", new { @class = "text-danger" })

                                        </div>

                                        <div class="col-sm-12 col-md-8" style="padding:0">
                                            <div class="table table-bordered pull-right" style="margin-bottom:0px">
                                                <div class="row">
                                                    <div class="col-xs-12 col-md-2">
                                                        <h1 class="rating-num" id="ratingheader"></h1>
                                                        <div class="rating" id="ratingdiv">

                                                        </div>
                                                        <div class="col-md-2">
                                                            <span id="ratingtotal" class="glyphicon glyphicon-user"></span>
                                                        </div>
                                                    </div>

                                                    <div class="col-sm-12 col-md-10" style="margin-top:10px">

                                                        <div style="margin-bottom:2px">
                                                            <div class="col-xs-4 col-md-1 text-right" style="font-size:8px">
                                                                <span class="glyphicon glyphicon-star" style="font-size:6px"></span>5
                                                            </div>
                                                            <div class="col-xs-8 col-md-11">
                                                                <div class="progress progress-striped">
                                                                    <div id="divspan5" class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="20"
                                                                         aria-valuemin="0" aria-valuemax="100">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!-- end 5 -->
                                                        <div style="margin-bottom:2px">
                                                            <div class="col-xs-4 col-md-1 text-right" style="font-size:8px">
                                                                <span class="glyphicon glyphicon-star" style="font-size:6px"></span>4
                                                            </div>
                                                            <div class="col-xs-8 col-md-11">
                                                                <div class="progress">
                                                                    <div id="divspan4" class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="20"
                                                                         aria-valuemin="0" aria-valuemax="100">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!-- end 4 -->
                                                        <div class="col-xs-4 col-md-1 text-right" style="font-size:8px">
                                                            <span class="glyphicon glyphicon-star" style="font-size:6px"></span>3
                                                        </div>
                                                        <div class="col-xs-8 col-md-11">
                                                            <div class="progress">
                                                                <div id="divspan3" class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="20"
                                                                     aria-valuemin="0" aria-valuemax="100">
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!-- end 3 -->
                                                        <div class="col-xs-4 col-md-1 text-right" style="font-size:8px">
                                                            <span class="glyphicon glyphicon-star" style="font-size:6px"></span>2
                                                        </div>
                                                        <div class="col-xs-8 col-md-11">
                                                            <div class="progress">
                                                                <div id="divspan2" class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="20"
                                                                     aria-valuemin="0" aria-valuemax="100">
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!-- end 2 -->
                                                        <div class="col-xs-4 col-md-1 text-right" style="font-size:8px">
                                                            <span class="glyphicon glyphicon-star" style="font-size:6px"></span>1
                                                        </div>
                                                        <div class="col-xs-8 col-md-11">
                                                            <div class="progress">
                                                                <div id="divspan1" class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="80"
                                                                     aria-valuemin="0" aria-valuemax="100">
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!-- end 1 -->

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>

                                <div class="col-md-3">
                                    <div class="list-group pull-right" id="feature" style="padding-top:10PX;padding-right:5px; width:100%; height:660px;overflow-y:scroll;">

                                        <a href="#" class="list-group-item list-group-item-action active" style="background-color:orange; border:none">MONTHLY BIRTHDAY'S</a>

                                        @foreach (var Resourcereportings in Model.CurrentBirthdays)
                                        {
                                            <a href="#" class="list-group-item list-group-item-action">

                                                <div class="d-flex w-100 justify-content-between">
                                                    <h5 class="mb-1"> @Resourcereportings</h5>
                                                    <small class="text-muted"> Happy Birthday!</small>
                                                </div>
                                            </a>
                                        }
                                    </div>
                                </div>
                            </form>
                        </div>

                    </div>

                    <div class="tab-pane fade" id="tab2default">
                        <div class="form-group row well">
                            <div class=" row col-md-4 " style="height:390px">
                                <div class="col-md-12" style="margin-top:15px;">
                                    <label class="col-sm-9 col-form-label col-form-label-sm" style="padding-left:0">Pay Frequency</label>
                                    @Html.DropDownListFor(model => model.payFrequenciesID, Model.PayFrequencyList, "Pay Frequency", new { @class = "form-control", id = "ddlPayFrequencyId" })
                                </div>
                                <div class="col-md-12" style="margin-top:15px;">
                                    <label class="col-sm-9 col-form-label col-form-label-sm" style="padding-left:0">ADP Employee Id</label>
                                    <input type="text" id="txtADPEmployeeId" class="form-control form-control-sm" placeholder="ADP Employee Id">
                                </div>
                                <div class="col-md-12" style="margin-top:15px;">
                                    <label class="col-sm-9 col-form-label col-form-label-sm" style="padding-left:0">Resource Assigned</label>
                                    @Html.DropDownListFor(model => model.AssignResourcesID, Model.AssignResourceList, "AssignResource", new { @class = "form-control", id = "ddlResourceAssigned" })
                                </div>
                                <div class="col-md-12" style="margin-top:15px;">
                                    <label class="col-sm-9 col-form-label col-form-label-sm" style="padding-left:0">Annual Salary</label>
                                    <input type="text" class="form-control form-control-sm" id="txtAnnualSalary" placeholder="Annual Salary">
                                </div>
                            </div>
                            <div class=" row col-md-4" style="height:390px">
                                <div class="col-md-12" style="margin-top:15px;">
                                    <label class="col-sm-9 col-form-label col-form-label-sm" style="padding-left:0">Daily Rate</label>
                                    <input type="text" class="form-control form-control-sm" id="txtDailyRate" placeholder="Daily Rate">
                                </div>
                                <div class="col-md-12" style="margin-top:15px;">
                                    <label class="col-sm-9 col-form-label col-form-label-sm" style="padding-left:0">Work Rights</label>
                                    @Html.DropDownListFor(model => model.WorkRights, Model.WorkRightsList, "Works Rights", new { @class = "form-control", id = "ddlWorkRights" })
                                </div>
                                <div class="col-md-12" style="margin-top:15px;">
                                    <label class="col-sm-9 col-form-label col-form-label-sm" style="padding-left:0">Employment Status</label>
                                    @Html.DropDownListFor(model => model.EmployeeStatusSetID, Model.EmployeeStatusList, "Employment Status", new { @class = "form-control", id = "ddlEmployeeStatusId" })
                                </div>
                                <div class="col-md-12" style="margin-top:15px;">
                                    <label class="col-sm-9 col-form-label col-form-label-sm" style="padding-left:0">Notice Period</label>
                                    <input type="text" class="form-control form-control-sm" id="txtNoticePeriod" placeholder="Notice Period">
                                </div>
                                @*<div class="col-md-12" style="margin-top:15px;">
                                        <label class="col-sm-9 col-form-label col-form-label-sm" style="padding-left:0">CandidateID</label>
                                        <input type="text" class="form-control form-control-sm" id="txtCandidateID" placeholder="Notice Period">
                                    </div>*@
                            </div>
                            <div class=" row col-md-4" style="height:390px">

                                <div class="col-md-8">
                                    <label>People Manager</label>
                                    <div class="input-group">
                                        <input type="checkbox" checked data-toggle="toggle" data-onstyle="warning" id="chkIsPeopleManager" data-on="Yes" data-off="No" onchange="PeopleManagerCBChange(this);">
                                    </div>
                                </div>
                                @*<div class="col-md-12" style="margin-top:15px;">
                                        @Html.DropDownListFor(model => Model.ResourceIDs, Model.CandidateNameList, new { @id = "CandidateNameId", @style = "height:10px; width:10px;", @multiple = "multiple" })
                                    </div>*@
                                <div class="col-md-4" style="padding-top:27px">
                                    <input type="submit" value="ASSIGN" class="btn btn-primary"
                                           style="background-color:orange;border:none;"
                                           id="updatePeopleManager" onclick="UpdatePeopleManager();" />
                                </div>

                                <div style="padding-left:15px; padding-right:15px;">
                                    <div class="list-group list-group-warning" style="padding-top:5px;padding-right:5px; height:315px;overflow-y:scroll; width:100%" id="reportingDiv">
                                    </div>
                                </div>

                            </div>
                        </div>

                        <div class="col-md-12" style="padding:0">

                            <fieldset style="margin-bottom:0px">
                                <div class="form-group row well" style="margin-bottom:20px">
                                    <div class="form-group row">
                                        <div class="col-md-3">
                                            <label>Address:</label>
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    <i class="glyphicon glyphicon-home"></i>
                                                </span>
                                                <input id="txtAddress1" placeholder="Address Line 1" class="form-control" required="true" value="" type="text">
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <label>City:</label>
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    <i class="fas fa-map-marker-alt"></i>
                                                </span>
                                                <input id="txtCity" placeholder="Suburb" class="form-control" required="true" value="" type="text">
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <label>State:</label>
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    <i class="glyphicon glyphicon-home"></i>
                                                </span>
                                                <input id="txtState" placeholder="State" class="form-control" required="true" value="" type="text">
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <label>Postal Code:</label>
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    <i class="glyphicon glyphicon-home"></i>
                                                </span>
                                                <input id="txtPostalCode" placeholder="PostCode" class="form-control" required="true" value="" type="text">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-md-3">

                                            <label>DOB</label>
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    <i class="glyphicon glyphicon-calendar"></i>
                                                </span>
                                                <input type="date" class="col-xs-1 form-control" id="txtDOB" placeholder="DOB" onchange="calculateAge(this);">
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <label>Age</label>
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    <i class="glyphicon glyphicon-calendar"></i>
                                                </span>
                                                <input type="text" class="form-control" id="txtAge" placeholder="Age">
                                            </div>

                                        </div>
                                        <div class="col-md-3">
                                            <label>Gender</label>
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    <i class="fas fa-user-friends"></i>
                                                </span>
                                                <select class="form-control" id="txtGender">
                                                    <option>Select</option>
                                                    <option>Male</option>
                                                    <option>Female</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <label>Marital Status</label>
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    <i class="fas fa-user-friends"></i>
                                                </span>
                                                <select class="form-control" id="txtStatus">
                                                    <option>Married</option>
                                                    <option>Single</option>
                                                </select>
                                            </div>
                                        </div>

                                    </div>

                                </div>
                            </fieldset>

                        </div>
                    </div>

                    <div class="tab-pane fade" id="tab3default">
                        <div class="row clearfix">
                            @Html.Partial("VerserSkills", Model)
                        </div>
                    </div>

                    <div class="tab-pane fade" id="tab4default" style="font:x-small">
